<div class="col-6 d-flex flex-column mt-2">
  <%= image_tag 'sky.jpeg', size: '500x400' %>
  <div class="bg-white border border-dark mt-2 p-2">
    <% memory.good_events.each do |event| %>
      <ul>
        <li>
          <%= link_to event.title, good_event_path(event), class:"text-black"%>
        </li>
      </ul>
    <% end %>
  </div>
</div>
