<div class="page d-flex align-items-center">
  <div class="page-inner mx-auto">
    <%= image_tag memory.bad_image.url, size: "50x50", class: "page-bad-img" %>
    <%= image_tag "arrow.png", size: "50x50", class: "arrow-img mt-2" %>
    <div class="text-center">
      <%= link_to image_tag(memory.good_image.url, size: "200x300", class: "page-good-img"), memory_path(memory) %>
    </div>
  </div>
</div>